{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.svg",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
    "breadcrumb_path": "/bcs/breadcrumb/toc.json",
		"_op_documentIdPathDepotMapping": {
			"./": {
			  "depot_name": "TechNet.bcs"
			}
		}
	},
    "fileMetadata": {},
    "template": [],
    "dest": "bcs"
  }
}